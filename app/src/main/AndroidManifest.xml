<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.laizexin.sdj.customui">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AutoScrollingTextViewActivity" />
        <activity android:name=".MarqueeTextViewActivity" />
        <activity android:name=".FlickerTextViewActivity" />
        <activity android:name=".StaticBlockActivity" />
        <activity android:name=".Bezier2Activity" />
        <activity android:name=".Bezier3Activity" />
        <activity android:name=".ScratchViewActivity" />
        <activity android:name=".BezierFavouriteActivity" />
        <activity
            android:name=".AdImageViewActivity"
            android:label="@string/title_activity_ad_image_view"></activity>
    </application>

</manifest>